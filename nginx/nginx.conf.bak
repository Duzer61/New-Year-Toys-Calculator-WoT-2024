server {
  listen 80;
  index index.html;
  server_tokens off;


  location /admin/ {
    proxy_set_header Host $http_host;
    proxy_pass http://calculator:8000/admin/;
  }

  location /media/ {
    proxy_set_header Host $http_host;
    alias /media/;
  }

  location / {
    proxy_set_header Host $http_host;
    alias /staticfiles/;
    try_files $uri $uri/ /index.html;
  }
}